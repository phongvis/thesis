@inproceedings{Jeong2008,
abstract = {It has been widely accepted that interactive visualization techniques enable users to more effectively form hypotheses and identify areas for more detailed investigation. There have been numerous empirical user studies testing the effectiveness of specific visual analytical tools. However, there has been limited effort in connecting a userpsilas interaction with his reasoning for the purpose of extracting the relationship between the two. In this paper, we present an approach for capturing and analyzing user interactions in a financial visual analytical tool and describe an exploratory user study that examines these interaction strategies. To achieve this goal, we created two visual tools to analyze raw interaction data captured during the user session. The results of this study demonstrate one possible strategy for understanding the relationship between interaction and reasoning both operationally and strategically.},
annote = {Jeong et al. use two tools to visualise and analyse user interaction logs to understand reasoning processes. 
First, the Operation Analysis tool shows the user interactions with WireVis over time during the analysis session. The tool consists of multiple timeline rows. Three first rows show the operations of the user with three WireVis{\&}{\#}039; views: Heatmap view, Strings and Beads view and Search by Example view. By observing these three rows, the usage pattern of the user can be revealed; for example, in the first five minutes, the analyst uses the Heatmap view and focuses on some high frequency keywords, then he switches to the Strings and Beads view to observe the transaction pattern of some interested accounts found in the Heatmap view. In the next two rows, the tool also shows how "deep" the user interact with the system (analyse the overview pattern or directly examine detail transactions) and how "wide" the user analyses the data (investigate the whole time span of the data or just focus on some suspicious transactions).
Second, the Strategy Analysis tool helps to discover the high level strategy that the user employs to detect suspicious activities. This tool shows the overall picture of the analysis process regardless of time instead of the step by step actions in the Operation Analysis tool. The Strategy Analysis tool is composed of three tree maps to show the data grouped by time, keyword and account. A cell in these tree maps depicts a transaction and the size of the circle inside the cell implies the time that the analyst spent to examine that transaction. For example, in [], the account tree map shows that the analyst focuses on two particular accounts (two rectangles with dominant large circles).},
author = {Jeong, Dong Hyun and Dou, Wenwen and Tukes, Felesia and Ribarsky, William and Lipford, Heather Richter and Chang, Remco},
booktitle = {IEEE Symposium on Visual Analytics Science and Technology},
doi = {10.1109/VAST.2008.4677360},
file = {:Users/phong/Documents/Mendeley Desktop/Jeong et al. - 2008 - Evaluating the relationship between user interaction and financial visual analysis.pdf:pdf},
pages = {83--90},
title = {{Evaluating the relationship between user interaction and financial visual analysis}},
url = {http://ieeexplore.ieee.org/xpl/freeabs{\_}all.jsp?arnumber=4677360},
year = {2008}
}
